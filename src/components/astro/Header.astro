---
const links = [
  { label: "Home", href: "/" },
  { label: "Profile", href: "/profile" },
  { label: "Dashboard", href: "/dashabord" },
];
---

<header class="bg-neutral-900 text-white flex sticky top-0 left-0 right-0 z-10">
  <div class="flex items-center">
    {
      links.map(({ label, href }) => (
        <a
          class="p-4 hover:bg-neutral-600 transition-all duration-200"
          href={href}
        >
          {label}
        </a>
      ))
    }
  </div>
</header>
